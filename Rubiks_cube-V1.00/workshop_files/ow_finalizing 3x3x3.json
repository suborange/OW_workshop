rule("give ult charge if its at 0")
{
	event
	{
		Ongoing - Each Player;
		Team 1;
		All;
	}

	conditions
	{
		Ultimate Charge Percent(Event Player) <= 99;
	}

	actions
	{
		Set Ultimate Charge(Event Player, 100);
		disabled Press Button(Event Player, Button(Ultimate));
		disabled Set Ultimate Ability Enabled(Event Player, True);
		disabled Create HUD Text(All Players(All Teams), Custom String("charging"), Null, Null, Left, 0, Color(Turquoise), Color(White), Color(
			White), Visible To and String, Default Visibility);
		If(Is Using Ultimate(Event Player) == True);
			Event Player.ULT = 1;
		End;
		If(Event Player.ULT == 1);
			Wait(15, Ignore Condition);
			Press Button(Event Player, Button(Ultimate));
	}
}

rule("simulation information")
{
	event
	{
		Ongoing - Each Player;
		Team 1;
		All;
	}

	conditions
	{
		Is Button Held(Event Player, Button(Reload)) == True;
	}

	actions
	{
		"toggle off, now toggle on."
		If(Event Player.text_toggle == 0);
			Destroy All HUD Text;
			Create HUD Text(All Players(All Teams), Custom String("Press 'reload' to toggle this pop up on/off"), Null, Null, Right, 0, Color(
				White), Color(White), Color(White), Visible To and String, Default Visibility);
			Create HUD Text(All Players(All Teams), Custom String("Use Mercy's Ultimate to fly around. Instant use every 15 seconds. "), Null,
				Null, Right, 0, Color(Orange), Color(White), Color(White), Visible To and String, Default Visibility);
			Create HUD Text(All Players(All Teams), Custom String("Try turning off background sounds for best immersion."), Null, Null, Right,
				0, Color(Rose), Color(White), Color(White), Visible To and String, Default Visibility);
			Create HUD Text(All Players(All Teams), Custom String("Look at each bot for each rubik's move notation."), Null, Null, Right, 0,
				Color(Blue), Color(White), Color(White), Visible To and String, Default Visibility);
			Create HUD Text(All Players(All Teams), Custom String("Shoot the bot to apply the corresponding move."), Null, Null, Right, 0,
				Color(Blue), Color(White), Color(White), Visible To and String, Default Visibility);
			Create HUD Text(All Players(All Teams), Custom String("Press 'melee' to toggle player HUD"), Null, Null, Right, 0, Color(White),
				Color(White), Color(White), Visible To and String, Default Visibility);
			disabled Wait(15, Ignore Condition);
			disabled Destroy All HUD Text;
			Create HUD Text(All Players(All Teams), Custom String("Press 'interact' to toggle between different camera angles"), Null, Null,
				Right, 0, Color(White), Color(White), Color(White), Visible To and String, Default Visibility);
			Create HUD Text(All Players(All Teams), Custom String("Press 'jump' to go back to default mercy view"), Null, Null, Right, 0,
				Color(White), Color(White), Color(White), Visible To and String, Default Visibility);
			"on - displaying HUD"
			Event Player.text_toggle = 1;
		"toggled on, so now toggle off ( delete all HUD)"
		Else If(Event Player.text_toggle == 1);
			Destroy All HUD Text;
			"off - no HUD displayed"
			Event Player.text_toggle = 0;
		End;
	}
}

rule("HUD toggle")
{
	event
	{
		Ongoing - Each Player;
		Team 1;
		All;
	}

	conditions
	{
		Is Button Held(Event Player, Button(Melee)) == True;
	}

	actions
	{
		If(Event Player.HUD_toggle == 0);
			Disable Hero HUD(Event Player);
			Event Player.HUD_toggle = 1;
			Create HUD Text(All Players(All Teams), Custom String("toggle HUD"), Null, Null, Left, 0, Color(Turquoise), Color(White), Color(
				White), Visible To and String, Default Visibility);
		Else If(Event Player.HUD_toggle == 1);
			Enable Hero HUD(Event Player);
			Event Player.HUD_toggle = 0;
			Create HUD Text(All Players(All Teams), Custom String("toggle HUD"), Null, Null, Left, 0, Color(Turquoise), Color(White), Color(
				White), Visible To and String, Default Visibility);
		End;
	}
}

rule("go through each camera view - interact button")
{
	event
	{
		Ongoing - Each Player;
		Team 1;
		All;
	}

	conditions
	{
		Is Button Held(Event Player, Button(Interact)) == True;
	}

	actions
	{
		Set Invisible(All Players(All Teams), All);
		Start Camera(Event Player, Event Player.Camera_view[Event Player.Camera_index], Vector(5, 15, 5), 0);
		Event Player.Camera_index += 1;
		If(Event Player.Camera_index == Count Of(Event Player.Camera_view));
			"reset back to 1, for next iteration of angles"
			Event Player.Camera_index = 1;
	}
}

rule("go back to mercy mode")
{
	event
	{
		Ongoing - Each Player;
		All;
		All;
	}

	conditions
	{
		Is Button Held(Event Player, Button(Jump)) == True;
	}

	actions
	{
		Stop Camera(Event Player);
		Event Player.Camera_index = 0;
		Set Invisible(All Players(All Teams), None);
	}
}

