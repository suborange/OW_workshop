		"RIGHT INVERSE BOT"
		Create Dummy Bot(Hero(Reinhardt), Team 2, 7, Vector(-0.927, 4.127, 22.691), Vector(0, 0, 0));
		"DOWN INVERSE"
		Create Dummy Bot(Hero(Widowmaker), Team 2, 11, Vector(8.160, 2.771, -1.091), Vector(0, 0, 0));
		"UP INVERSE"
		disabled Create Dummy Bot(Hero(Junkrat), Team 2, 8, Vector(14.877, 2.477, 3.986), Vector(0, 0, 0));
		"LEFT INVERSE"
		disabled Create Dummy Bot(Hero(LÃºcio), Team 2, 10, Vector(14.950, 2.477, 11.418), Vector(0, 0, 0));
		"BACK INVERSE"
		disabled Create Dummy Bot(Hero(Baptiste), Team 2, 9, Vector(14.958, 2.483, 8.100), Vector(0, 0, 0));
		"rename"
		Start Forcing Dummy Bot Name(All Players(Team 2)[6], Custom String("FRONT INVERSE"));
		Start Forcing Dummy Bot Name(All Players(Team 2)[7], Custom String("RIGHT INVERSE"));
		Start Forcing Dummy Bot Name(All Players(Team 2)[8], Custom String("DOWN INVERSE"));
		disabled Start Forcing Dummy Bot Name(All Players(Team 2)[9], Custom String("UP INVERSE"));
		disabled Start Forcing Dummy Bot Name(All Players(Team 2)[10], Custom String("LEFT INVERSE"));
		disabled Start Forcing Dummy Bot Name(All Players(Team 2)[11], Custom String("BACK INVERSE"));
		"for each bot, make gravity 0"
		For Global Variable(gravity_counter, 0, Count Of(All Players(Team 2)), 1);
			Set Gravity(All Players(Team 2)[Global.gravity_counter], 0);
		End;
		Start Scaling Player(All Players(Team 2), 2.500, False);
		Disable Movement Collision With Environment(All Players(Team 2), True);
	}
}

