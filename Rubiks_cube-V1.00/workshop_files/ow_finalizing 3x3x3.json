rule("player starts")
{
	event
	{
		Ongoing - Each Player;
		Team 1;
		All;
	}

	conditions
	{
		"run once when spawned in"
		Has Spawned(Event Player) == True;
	}

	actions
	{
		Wait(0.250, Ignore Condition);
		Set Gravity(Event Player, 0);
		Set Invisible(Event Player, All);
		disabled Set Move Speed(Event Player, 70);
		"default to toggle on"
		Event Player.HUD_toggle = 0;
		"default to toggle off"
		Event Player.ULT = 0;
		Event Player.text_toggle = 0;
		"set to 0; false. not dead yet."
		Global.is_bot_dead = 0;
		Create HUD Text(All Players(All Teams), Custom String("Press 'reload' to read all information on this game mode"), Null, Null,
			Right, 0, Color(Rose), Color(White), Color(White), Visible To and String, Default Visibility);
		disabled Create HUD Text(All Players(All Teams), Custom String("Use Mercy's Ultimate to fly around, instantly re-used after 15 seconds. "),
			Null, Null, Right, 0, Color(Rose), Color(White), Color(White), Visible To and String, Default Visibility);
		disabled Create HUD Text(All Players(All Teams), Custom String("Try turning off background sounds for best immersion."), Null, Null, Right,
			0, Color(Rose), Color(White), Color(White), Visible To and String, Default Visibility);
		disabled Create HUD Text(All Players(All Teams), Custom String("Look at each bot for each rubik's move notation."), Null, Null, Right, 0,
			Color(Rose), Color(White), Color(White), Visible To and String, Default Visibility);
		disabled Create HUD Text(All Players(All Teams), Custom String("Press 'melee' to toggle player HUD"), Null, Null, Right, 0, Color(Rose),
			Color(White), Color(White), Visible To and String, Default Visibility);
		Disable Game Mode HUD(Event Player);
		Disable Movement Collision With Players(Event Player);
		disabled Modify Global Variable(const, Append To Array, Vector(-5, 5, -5));
		disabled Modify Global Variable(const, Append To Array, Vector(-5, 5, 1));
		disabled Modify Global Variable(const, Append To Array, Vector(-2, 5, -5));
		disabled Modify Global Variable(const, Append To Array, Vector(-2, 5, 1));
		disabled Modify Global Variable(is_bot_dead, Append To Array, Global.const[1]);
