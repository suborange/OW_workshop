rule("teleport players to starting location")
{
	event
	{
		Ongoing - Each Player;
		Team 1;
		All;
	}

	conditions
	{
		Has Spawned(Event Player) == True;
	}

	actions
	{
		Teleport(All Players(Team 1), Vector(-29.871, 23.755, -9.829));
		Wait(0.100, Ignore Condition);
		Set Facing(Event Player, Vector(10, -2, 10), To World);
	}
}

rule("START spawnbots")
{
	event
	{
		Ongoing - Global;
	}

	conditions
	{
		Is Game In Progress == True;
	}

	actions
	{
		"DUMMIES - FRONT BOT"
		Create Dummy Bot(Hero(Doomfist), Team 2, 0, Vector(-6.293, 3, 7.196), Vector(0, 0, 0));
		"RIGHT BOT"
		Create Dummy Bot(Hero(Roadhog), Team 2, 1, Vector(-0.408, 0.966, 14.430), Vector(0, 0, 0));
		"UP BOT"
		Create Dummy Bot(Hero(Junker Queen), Team 2, 2, Vector(3.397, 32, 11.666), Vector(0, 0, 0));
		"BACK BOT"
